#
# Concepts
#

:Subscription
  a owl:Class ;
  rdfs:label "Subscription"@en ;
  rdfs:comment "A unique relation to a topic by a subscriber that indicates it should receive updates for that topic."@en ;
  rdfs:isDefinedBy : ;
  dct:issued "2016-07-08"^^xsd:date ;
  dct:modified "2016-07-08"^^xsd:date ;
  vs:term_status "testing"@en ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :topic ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :callback ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :secret ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
  ] ;
.

:Subscriber
  a owl:Class ;
  rdfs:label "Subscriber"@en ;
  rdfs:comment "An entity that wants to be notified of changes on a topic"@en ;
  rdfs:isDefinedBy : ;
  dct:issued "2016-07-08"^^xsd:date ;
  dct:modified "2016-07-08"^^xsd:date ;
  vs:term_status "testing"@en ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty dct:identifier ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] ;
.

#
# Relations
#

:topic
  a owl:DatatypeProperty ;
  rdfs:label "topic URL"@en ;
  rdfs:comment "An HTTP [RFC2616] resource URL for which updates should be published."@en ;
  rdfs:isDefinedBy : ;
  rdfs:domain :Subscription ;
  rdfs:range xsd:anyURI ;
  dct:issued "2016-07-08"^^xsd:date ;
  dct:modified "2016-07-08"^^xsd:date ;
  vs:term_status "testing"@en ;
.

:callback
  a owl:DatatypeProperty ;
  rdfs:label "callback URL"@en ;
  rdfs:comment "The URL [RFC3986] at which a subscriber wishes to receive notifications."@en ;
  rdfs:isDefinedBy : ;
  rdfs:domain :Subscription ;
  rdfs:range xsd:anyURI ;
  dct:issued "2016-07-08"^^xsd:date ;
  dct:modified "2016-07-08"^^xsd:date ;
  vs:term_status "testing"@en ;
.

:secret
  a owl:DatatypeProperty ;
  rdfs:label "secret"@en ;
  rdfs:comment "A secret string that will be used to compute an HMAC digest for authorized content distribution. If not supplied, the HMAC digest will not be present for content distribution requests."@en ;
  rdfs:isDefinedBy : ;
  rdfs:domain :Subscription ;
  rdfs:range xsd:string ;
  dct:issued "2016-07-08"^^xsd:date ;
  dct:modified "2016-07-08"^^xsd:date ;
  vs:term_status "testing"@en ;
.

