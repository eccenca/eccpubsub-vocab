:
  a owl:Ontology , voaf:Vocabulary ;
  rdfs:label "eccpubsub: eccenca Publish-Subscribe Vocabulary"@en ;
  rdfs:comment "The eccenca Publish-Subscribe Vocabulary defines concepts and relations to create statements about publishers, subscribers and their subscriptions in a Publish-Subscribe environment based on the PubSubHubbub Core 0.4 specification."@en ;
  rdfs:isDefinedBy : ;
  dct:issued "2016-07-12"^^xsd:date ;
  dct:modified "2016-12-01"^^xsd:date ;
  owl:versionInfo "SNAPSHOT" ;
  dct:rights "Copyright Â© 2016 eccenca GmbH" ;
  dct:publisher ecc:eccenca-GmbH ;
  <http://creativecommons.org/ns#license> <http://creativecommons.org/licenses/by/3.0/> ;
  dct:creator aksw:MarvinFrommhold, <http://sebastian.tramp.name> ;
  void:vocabulary
    <http://purl.org/dc/terms/> ,
    <http://purl.org/vocab/vann/> ,
    <http://www.w3.org/2003/06/sw-vocab-status/ns#> ,
    <http://xmlns.com/foaf/0.1/> ;
  vann:preferredNamespacePrefix "eccpubsub" ;
  vann:preferredNamespaceUri "https://vocab.eccenca.com/pubsub/" ;
  doap:implements <http://pubsubhubbub.github.io/PubSubHubbub/pubsubhubbub-core-0.4.html> ;
  doap:homepage <https://github.com/eccenca/eccpubsub-vocab> ;
  doap:bug-database <https://github.com/eccenca/eccpubsub-vocab/issues> ;
  doap:download-page <https://github.com/eccenca/eccpubsub-vocab/releases> ;
  doap:repository <https://github.com/eccenca/eccpubsub-vocab.git> ;
.

ecc:eccenca-GmbH
  a foaf:Organization, dct:Agent ;
  foaf:name "eccenca GmbH" ;
.

aksw:MarvinFrommhold
  a foaf:Person, dct:Agent ;
  foaf:name "Marvin Frommhold" ;
.

<http://sebastian.tramp.name>
  a foaf:Person , dct:Agent ;
  foaf:name "Sebastian Tramp" ;
.

<http://creativecommons.org/licenses/by/3.0/>
  rdfs:label "Creative Commons Attribution 3.0 Unported" ;
.

<http://pubsubhubbub.github.io/PubSubHubbub/pubsubhubbub-core-0.4.html> a doap:Specification;
    rdfs:label "PubSubHubbub Core 0.4 -- Working Draft" ;
.

#
# Concepts
#

:Subscription
  a owl:Class ;
  rdfs:label "subscription"@en ;
  rdfs:comment "A unique relation to a topic by a subscriber that indicates it wants to receive updates for that topic."@en ;
  rdfs:isDefinedBy : ;
  dct:issued "2016-07-12"^^xsd:date ;
  dct:modified "2016-12-01"^^xsd:date ;
  vs:term_status "testing"@en ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty dct:created ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty dct:modified ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :topic ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hub ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :secret ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
  ] ;
.

:SubscriberSubscription
  a owl:Class ;
  rdfs:subClassOf :Subscription ;
  rdfs:label "subscriber subscription"@en ;
  rdfs:comment "Representation of a subscription on the subscriber side."@en ;
  rdfs:isDefinedBy : ;
  dct:issued "2016-12-01"^^xsd:date ;
  dct:modified "2016-12-01"^^xsd:date ;
  vs:term_status "testing"@en ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :state ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :deniedReason ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
  ] ;
.

:PublisherSubscription
  a owl:Class ;
  rdfs:subClassOf :Subscription ;
  rdfs:label "publisher subscription"@en ;
  rdfs:comment "Representation of a subscription on the publisher side."@en ;
  rdfs:isDefinedBy : ;
  dct:issued "2016-12-01"^^xsd:date ;
  dct:modified "2016-12-01"^^xsd:date ;
  vs:term_status "testing"@en ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :callback ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] ;
.

:GraphSubscription
  a owl:Class ;
  rdfs:subClassOf :SubscriberSubscription ;
  rdfs:label "graph subscription"@en ;
  rdfs:comment "A subscription to an RDF graph."@en ;
  rdfs:isDefinedBy : ;
  dct:issued "2016-08-26"^^xsd:date ;
  dct:modified "2016-08-26"^^xsd:date ;
  vs:term_status "testing"@en ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :targetGraph ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :targetEndpointId ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] ;
.

:Endpoint
  a owl:Class ;
  rdfs:label "endpoint"@en ;
  rdfs:comment "A PubSubHubbub HTTP endpoint. Individuals of this class MUST be referenced by their URL."@en ;
  rdfs:isDefinedBy : ;
  dct:issued "2016-07-12"^^xsd:date ;
  dct:modified "2016-07-12"^^xsd:date ;
  vs:term_status "testing"@en ;
.

:CallbackEndpoint
  a owl:Class ;
  rdfs:subClassOf :Endpoint ;
  rdfs:label "callback endpoint"@en ;
  rdfs:comment "An HTTP endpoint acting as callback of a subscriber."@en ;
  rdfs:isDefinedBy : ;
  dct:issued "2016-07-12"^^xsd:date ;
  dct:modified "2016-07-12"^^xsd:date ;
  vs:term_status "testing"@en ;
.

:HubEndpoint
  a owl:Class ;
  rdfs:subClassOf :Endpoint ;
  rdfs:label "hub endpoint"@en ;
  rdfs:comment "An HTTP endpoint acting as hub."@en ;
  rdfs:isDefinedBy : ;
  dct:issued "2016-07-12"^^xsd:date ;
  dct:modified "2016-07-12"^^xsd:date ;
  vs:term_status "testing"@en ;
.

:SubscriptionState
  a owl:Class ;
  rdfs:label "subscription state"@en ;
  rdfs:comment "A state of a subscription."@en ;
  rdfs:isDefinedBy : ;
  dct:issued "2016-07-12"^^xsd:date ;
  dct:modified "2016-08-26"^^xsd:date ;
  vs:term_status "testing"@en ;
  owl:equivalentClass [
   a owl:Class ;
   owl:oneOf ( :SUBSCRIPTION_INITIATED :SUBSCRIPTION_SUBSCRIBED :SUBSCRIPTION_UNSUBSCRIBED :SUBSCRIPTION_DENIED )
 ]
.

#
# Relations
#

:state
  a owl:ObjectProperty ;
  rdfs:label "has subscription state"@en ;
  rdfs:comment "The state of a subscriber subscription."@en ;
  rdfs:isDefinedBy : ;
  rdfs:domain :SubscriberSubscription ;
  rdfs:range :SubscriptionState ;
  dct:issued "2016-07-12"^^xsd:date ;
  dct:modified "2016-12-01"^^xsd:date ;
  vs:term_status "testing"@en ;
.

:topic
  a owl:ObjectProperty ;
  rdfs:label "has topic URI"@en ;
  rdfs:comment "The topic URI for which updates should be published."@en ;
  rdfs:isDefinedBy : ;
  rdfs:domain :Subscription ;
  rdfs:range owl:Thing ;
  dct:issued "2016-07-12"^^xsd:date ;
  dct:modified "2016-08-26"^^xsd:date ;
  vs:term_status "testing"@en ;
.

:callback
  a owl:ObjectProperty ;
  rdfs:label "has callback URL"@en ;
  rdfs:comment "The callback URL at which a subscriber wishes to receive notifications."@en ;
  rdfs:isDefinedBy : ;
  rdfs:domain :PublisherSubscription ;
  rdfs:range :CallbackEndpoint ;
  dct:issued "2016-07-12"^^xsd:date ;
  dct:modified "2016-12-01"^^xsd:date ;
  vs:term_status "testing"@en ;
.

:hub
  a owl:ObjectProperty ;
  rdfs:label "has hub URL"@en ;
  rdfs:comment "The URL of the hub to which the request is addressed."@en ;
  rdfs:isDefinedBy : ;
  rdfs:domain :Subscription ;
  rdfs:range :HubEndpoint ;
  dct:issued "2016-07-12"^^xsd:date ;
  dct:modified "2016-08-26"^^xsd:date ;
  vs:term_status "testing"@en ;
.

:secret
  a owl:DatatypeProperty ;
  rdfs:label "has secret"@en ;
  rdfs:comment "A secret string that will be used to compute an HMAC digest for authorized content distribution. If not supplied, the HMAC digest will not be present for content distribution requests."@en ;
  rdfs:isDefinedBy : ;
  rdfs:domain :Subscription ;
  rdfs:range xsd:string ;
  dct:issued "2016-07-12"^^xsd:date ;
  dct:modified "2016-07-12"^^xsd:date ;
  vs:term_status "testing"@en ;
.

:deniedReason
  a owl:DatatypeProperty ;
  rdfs:label "has reason for denial"@en ;
  rdfs:comment "The reason for which the subscription has been denied."@en ;
  rdfs:isDefinedBy : ;
  rdfs:domain :Subscription ;
  rdfs:range xsd:string ;
  dct:issued "2016-07-12"^^xsd:date ;
  dct:modified "2016-08-26"^^xsd:date ;
  vs:term_status "testing"@en ;
.

:targetGraph
  a owl:ObjectProperty ;
  rdfs:label "target graph"@en ;
  rdfs:comment "The URI of the graph where the updates will be applied to."@en ;
  rdfs:isDefinedBy : ;
  rdfs:domain :GraphSubscription ;
  rdfs:range owl:Thing ;
  dct:issued "2016-08-26"^^xsd:date ;
  dct:modified "2016-08-26"^^xsd:date ;
  vs:term_status "testing"@en ;
.

:targetEndpointId
  a owl:DatatypeProperty ;
  rdfs:label "target endpoint ID"@en ;
  rdfs:comment "The ID of the endpoint where the target graph is contained."@en ;
  rdfs:isDefinedBy : ;
  rdfs:domain :GraphSubscription ;
  rdfs:range xsd:string ;
  dct:issued "2016-08-26"^^xsd:date ;
  dct:modified "2016-08-26"^^xsd:date ;
  vs:term_status "testing"@en ;
.

#
# Individuals
#

:SUBSCRIPTION_INITIATED
  a :SubscriptionState ;
  rdfs:label "initiated"@en ;
  rdfs:comment "Indicates that a subscription was successfully initiated but it is not yet verified by the hub."@en ;
  rdfs:isDefinedBy : ;
  dct:issued "2016-07-12"^^xsd:date ;
  dct:modified "2016-12-01"^^xsd:date ;
  vs:term_status "testing"@en ;
.

:SUBSCRIPTION_SUBSCRIBED
  a :SubscriptionState ;
  rdfs:label "subscribed"@en ;
  rdfs:comment "Indicates that a subscription was successfully validated and verified."@en ;
  rdfs:isDefinedBy : ;
  dct:issued "2016-08-26"^^xsd:date ;
  dct:modified "2016-08-26"^^xsd:date ;
  vs:term_status "testing"@en ;
.

:SUBSCRIPTION_UNSUBSCRIBED
  a :SubscriptionState ;
  rdfs:label "unsubscribed"@en ;
  rdfs:comment "Indicates that a subscriber unsubscribed from a subscription."@en ;
  rdfs:isDefinedBy : ;
  dct:issued "2016-08-26"^^xsd:date ;
  dct:modified "2016-08-26"^^xsd:date ;
  vs:term_status "testing"@en ;
.

:SUBSCRIPTION_DENIED
  a :SubscriptionState ;
  rdfs:label "denied"@en ;
  rdfs:comment "Indicates that a subscription was denied by a publisher."@en ;
  rdfs:isDefinedBy : ;
  dct:issued "2016-07-12"^^xsd:date ;
  dct:modified "2016-07-12"^^xsd:date ;
  vs:term_status "testing"@en ;
.

